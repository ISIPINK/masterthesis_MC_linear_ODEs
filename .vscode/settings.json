{
  "python.analysis.typeCheckingMode": "off",
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.formatting.provider": "none",
  "grammarly.selectors": [
    {
      "language": "latex",
      "scheme": "file",
      "pattern": "latex/main paper/main.tex"
    },
    {
      "language": "latex",
      "scheme": "file",
      "pattern": "latex/main paper/english_abstract.tex"
    }
  ]
}
